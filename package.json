    "url": "git+https://github.com/drom/jsof.git"
  },
  "keywords": [
    "JSON"
  ],
  "author": "alex.drom@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drom/jsof/issues"
  },
  "homepage": "https://github.com/drom/jsof#readme",
  "devDependencies": {
    "@drom/eslint-config": "^0.11.0",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "eslint": "^8.5.0",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.1"
  },
  "dependencies": {
    "escodegen": "^2.0.0",
    "esprima": "^4.0.1",
    "shift-codegen": "^8.1.0",
