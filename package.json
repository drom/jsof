{
  "name": "jsof",
  "version": "0.5.0",
  "description": "liberal JSON",
  "main": "lib",
  "scripts": {
    "test": "eslint lib test && nyc --reporter=html --reporter=text mocha test",
    "mocha": "mocha test",
    "coverage": "nyc report --reporter=lcov",
    "browserify": "browserify --standalone jsof lib/index1.js > build/jsof.js",
